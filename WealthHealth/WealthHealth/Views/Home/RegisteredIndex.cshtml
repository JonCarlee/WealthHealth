            });
            var first = false;
            function toLogin(number) {
                switch (number) {
                    case 1:
                        if (first == false) {
                            first = true;
                            $("#return").fadeIn(300);
                            break;
                        }
                        $("#create").slideUp(300);
                        $("#return").delay(300).slideDown(300);
                        break;
                    case 2:
                        if (first == false) {
                            first = true;
                            $("#create").fadeIn(300);
                            break;
                        }
                        $("#return").slideUp(300);
                        $("#create").delay(300).slideDown(300);
                        break
                }
